{
  "name": "cart_project",
  "version": "1.0.0",
  "description": "Cart Project",
  "author": "Bruno Cardoso",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:services": "cd docker && docker-compose up --no-start && cd ..",
    "start:services": "cd docker && docker-compose start && cd ..",
    "start:service:mongodb": "cd development-docker && docker start mongo-3.6.18 && cd ..",
    "start:service:rabbitmq": "cd docker && docker start rabbitmq-3.8.3 && docker exec rabbitmq-3.8.3 && cd ..",
    "stop:services": "cd docker && docker-compose stop && cd ..",
    "stop:service:mongodb": "cd docker && docker stop mongo-3.6.18 && cd ..",
    "stop:service:rabbitmq": "cd docker && docker stop rabbitmq-3.8.3 && cd ..",
    "build:microservices": "nest build api; nest build cart; nest build logs",
    "build:all": "npm run build:microservices",
    "lint:format": "eslint '{lib,microservices,config,api}/**/**/**/**/*.ts' --fix; prettier --write \"lib/**/**/**/**/*.ts\" \"api/**/**/**/*.ts\" \"config/**/**/**/*.ts\" \"microservices/**/**/**/**/*.ts\"",
    "start:api": "nest start api --watch",
    "start:cart": "nest start cart --watch",
    "start:logs": "nest start logs --watch",
    "start:all": "pm2 start ecosystem.microservices.config.js",
    "start:production": "pm2 start ecosystem.microservices.config.js --no-daemon",
    "stop:production": "env pm2 stop ecosystem.microservices.config.js; pm2 flush",
    "restart:production": "env pm2 restart ecosystem.microservices.config.js; pm2 flush",
    "reload:production": "env pm2 reload ecosystem.microservices.config.js; pm2 flush",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit:cov": "jest --coverage --runInBand --detectOpenHandles --no-cache --config ./jest.config.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.9.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/elasticsearch": "^8.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^9.3.9",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "amqp-connection-manager": "^4.1.11",
    "amqplib": "^0.10.3",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "helmet": "^6.0.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.7.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pm2": "^5.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.12",
    "webpack": "^5.76.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "_moduleAliases": {
    "@lib/decorators": "lib/src/decorators",
    "@lib/dto/general": "lib/src/dto/general",
    "@lib/enum": "lib/src/enum",
    "@lib/filter": "lib/src/filter",
    "@lib/type": "lib/src/type",
    "@lib/validators": "lib/src/validators",
    "@lib": "lib/src",
    "@config": "config/services",
    "@api": "api/src"
  },
  "_moduleDirectories": [
    "node_modules"
  ]
}
